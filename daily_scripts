limit->Draw("2*deltaNLL:r", "deltaNLL>0 && 2*deltaNLL<1", "L")
limit->Draw("JES:r", "deltaNLL>0 && 2*deltaNLL<1", "L")
limit->Draw("JER:r", "deltaNLL>0 && 2*deltaNLL<1", "L")
limit->Draw("TES:r", "deltaNLL>0 && 2*deltaNLL<1", "L")


