foreach dtag (`cat sw-dir/merge-sets/jobs_hadd.dtags.all-tt-systs_main`)
foreach dtag (`cat sw-dir/merge-sets/jobs_hadd.dtags.small_main`)
foreach dtag (`cat sw-dir/merge-sets/jobs_hadd.dtags.large_aa_main`)
