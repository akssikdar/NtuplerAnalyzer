ratio_bFrag = 0.955

ratio_bFrag = 0.97
ratio_Peterson = 0.991

cor_ratio(sf, int_var, int_nom) = (sf*(int_var/int_nom) - 1)/(int_var/int_nom - 1)

cor_ratio(0.97, 6246.32, 6383.36)
cor_ratio(0.97, 6497.12, 6383.36)

