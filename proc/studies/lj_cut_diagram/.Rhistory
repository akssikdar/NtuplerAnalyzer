cuts = read.csv("data.csv", header=T)
cuts
S = 36512.8
B = 16285.8
cuts$S2 = S - cuts$S1                                                                                                                                                                                                                                                          
cuts$B2 = B - cuts$B1                                                                                                                                                                                                                                                          
library(ggplot2)                                                                                                                                                                                                                                                               
cuts$C1 = cuts$S1 + cuts$B1
cuts$C2 = cuts$S2 + cuts$B2
cuts
cuts$C1 + cuts$C2
ggplot(cuts, (abs(C1-C2), abs(S1-B1))) + ggplot()
ggplot(cuts, aes(abs(C1-C2), abs(S1-B1))) + ggplot()
ggplot(cuts, aes(abs(C1-C2), abs(S1-B1)))
ggplot(cuts, aes(abs(C1-C2), abs(S1-B1)))
ggplot(cuts, aes(abs(C1-C2), abs(S1-B1))) + geom_point()
ggplot(cuts, (abs(C1-C2), abs(S1-B1))) + geom_text(label=cuts$cut)                                                                                                                                                                                                             
ggplot(cuts, aes(abs(C1-C2), abs(S1-B1))) + geom_text(label=cuts$cut)
ggplot(cuts, aes(abs(C1-C2), S1/B1)) + geom_text(label=cuts$cut)                                                                                                                                                                                                               
ggplot(cuts, aes(abs(C1-C2), abs(S2-B2))) + geom_text(label=cuts$cut)                                                                                                                                                                                                          
ggplot(cuts, aes(abs(C1-C2), abs(S1-B1))) + geom_text(label=cuts$cut)
ggplot(cuts, aes(abs(C1-C2), S1/B1 - S2/B2)) + geom_text(label=cuts$cut)
ggplot(cuts, aes(abs(C1-C2), S1/B1)) + geom_text(label=cuts$cut)
ggplot(cuts, aes(abs(C1-C2), S2/B2)) + geom_text(label=cuts$cut)
cuts
ggplot(cuts, aes(abs(C1-C2), S2-B2)) + geom_line()
ggplot(cuts, aes(C1-C2, S2-B2)) + geom_line()
ggplot(cuts, aes(C1-C2, S2-B2)) + geom_text(label=cuts$cut)
ggplot(cuts, aes(C1-C2, (S2-B2)/C2)) + geom_text(label=cuts$cut)
ggplot(cuts, aes(C1-C2, (S2-B2)/C2)) + geom_text(label=cuts$cut) + geom_text(mapping=aes(x=C1-C2, y = (S1-B1)/C1, label=cuts$cut))                                                                                                                                             
ggplot(cuts, aes(C1-C2, (S2-B2)/C2)) + geom_text(label=cuts$cut) + geom_text(mapping=aes(x=C1-C2, y = (B1-S1)/C1, label=cuts$cut))
ggplot(cuts, aes(C1-C2, (S2-B2)/C2)) + geom_text(label=cuts$cut) + geom_text(mapping=aes(x=C1-C2, y = (B1-S1)/C1, label=cuts$cut), color="red")
ggplot(cuts, aes(C1-C2, S2/S-B2/B)) + geom_text(label=cuts$cut)                                                                                                                                                                                                                
ggplot(cuts, aes(C1-C2, S2/S-B2/B)) + geom_text(label=cuts$cut)
ggplot(cuts, aes(abs(C1-C2), S2/S-B2/B)) + geom_text(label=cuts$cut)
ggplot(cuts, aes(C1-C2, S2/S-B2/B)) + geom_text(label=cuts$cut)
ggplot(cuts, aes(C1-C2, S2/S-B2/B)) + geom_text(label=cuts$cut) + geom_text(mapping=aes(x=C1-C2, y=B1/B-S1/S, label=cuts$cut), color="red")                                                                                                                                    
ggplot(cuts, aes(C1-C2, S2/S-B2/B)) + geom_text(label=cuts$cut) + geom_text(mapping=aes(x=C1-C2, y=S1/S-B1/B, label=cuts$cut), color="red")
ggplot(cuts, aes(C1-C2, S2/S-B2/B)) + geom_text(label=cuts$cut) + geom_text(mapping=aes(x=C2-C1, y=S1/S-B1/B, label=cuts$cut), color="red")                                                                                                                                    
ggplot(cuts, aes(C1-C2, S2/C2-B1/C1)) + geom_text(label=cuts$cut)                                                                                                                                                                                                              
ggplot(cuts, aes(C1-C2, S2/C2/(B1/C1))) + geom_text(label=cuts$cut)
ggplot(cuts, aes(abs(C1-C2), S2/C2/(B1/C1))) + geom_text(label=cuts$cut)
ggplot(cuts, aes(C1-C2, S1-B1)) + geom_text(label=cuts$cut)
q()
ggplot(cuts, aes(C1-C2, (S2-B2)/(S1-B1))) + geom_text(label=cuts$cut)                                                                                                                                                                                                          
library(ggplot2)
ggplot(cuts, aes(C1-C2, (S2-B2)/(S1-B1))) + geom_text(label=cuts$cut)
q()
library(ggplot2)
ggplot(cuts, aes(C1-C2, (S1/S)*(S2/S) + (B1/B)*(B2/B))) + geom_text(label=cuts$cut)                                                                                                                                                                                            
q()
